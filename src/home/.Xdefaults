!*.termName: xterm-256color

*saveLines: 32768
*scrollBar: false
*scrollBar.width: 8

*selectToClipboard: false

*translations: #override \n\
	Ctrl Shift <Key>C: copy-selection(CLIPBOARD) \n\
	Ctrl Shift <Key>V: insert-selection(CLIPBOARD)

*foreground: white
*background: black

!*faceName: DejaVu Sans Mono:size=8:antialias=true
*font: xft:DejaVu\ Sans\ Mono\ for\ Powerline:antialias=true
*faceSize: 8
*.scaleHeight: 1.0
*utf8: 1

Xcursor.theme: Obsidian
